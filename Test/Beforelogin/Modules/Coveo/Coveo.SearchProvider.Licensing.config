<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:coveo="http://www.sitecore.net/xmlconfig/coveo/">
  <sitecore coveo:require="!disabled">
    <commands>
      <command name="coveo:OpenIndexingManager" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenIndexingManagerCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenConfigurationManager" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenConfigurationManagerCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenDiagnosticPage" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenDiagnosticPageCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenAboutDialog" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenAboutDialogCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenPostInstallationWizard" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenPostInstallationWizardCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenResourcesDialog" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenResourcesDialogCommand, Coveo.SearchProvider.Applications" />
      <command name="coveo:OpenRelevanceManager" type="Coveo.SearchProvider.Applications.Shell.Commands.OpenrelevanceManagerCommand, Coveo.SearchProvider.Applications" />
    </commands>
    <controlSources>
      <source mode="on" namespace="Coveo.SearchProvider.Applications.HtmlControls" assembly="Coveo.SearchProvider.Applications" prefix="coveohtml" />
      <source mode="on" namespace="Coveo.SearchProvider.Applications.HtmlControls.OAuth" assembly="Coveo.SearchProvider.Applications" prefix="oauth" />
    </controlSources>
    <ui>
      <references>
        <reference id="Coveo.SearchProvider.Applications.dll">/bin/Coveo.SearchProvider.Applications.dll</reference>
      </references>
    </ui>
  </sitecore>
</configuration>